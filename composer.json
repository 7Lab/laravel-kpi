{
    "name": "7lab/laravel-kpi",
    "description": "Measure KPI statistics in a Laravel application",
    "keywords": [
        "7lab",
        "laravel-kpi",
        "laravel",
        "kpi"
    ],
    "homepage": "https://github.com/7lab/laravel-kpi",
    "license": "MIT",
    "authors": [
        {
            "name": "Joey Houtenbos",
            "email": "joey@7lab.nl",
            "homepage": "https://www.7lab.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "SevenLab\\Kpi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLab\\Kpi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SevenLab\\Kpi\\KpiServiceProvider"
            ]
        }
    }
}
