{
    "name": "7lab/laravel-kpi",
    "description": "Measure KPI statistics in a Laravel application",
    "keywords": [
        "7lab",
        "laravel-kpi",
        "laravel",
        "kpi"
    ],
    "homepage": "https://github.com/7lab/laravel-kpi",
    "license": "MIT",
    "authors": [
        {
            "name": "Joey Houtenbos",
            "email": "joey@7lab.nl",
            "homepage": "https://www.7lab.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/console": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "laravel/framework": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "fzaninotto/faker": "1.9.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "phpunit/phpunit": "^6.5.4|^7.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "SevenLab\\Kpi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLab\\Kpi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SevenLab\\Kpi\\KpiServiceProvider"
            ]
        }
    }
}
